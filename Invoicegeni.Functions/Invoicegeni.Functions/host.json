{
  "version": "2.0",
  "concurrency": {
    "dynamicConcurrencyEnabled": true
  },
  "extensions": {
    "blobs": {
      "maxDegreeOfParallelism": 8,
      "batchSize": 16,
      "newBatchThreshold": 8,
      "maxDequeueCount": 4 // Retry up to 5 times
    }
  },
  "logging": {
    "logLevel": {
      "default": "Information",
      "Function.ExtractInvoiceData": "Information",
      "Function": "Information",
      "Host.Results": "Information",
      "Host.Aggregator": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5
      },
      "enableLiveMetricsFilters": true
    }
  }
}